    <script>
        function showAnswer(e, crossId, answerId, questionRect) {
            e.target.style.display = 'none'
            document.getElementById(crossId).style.display = 'block'
            document.getElementById(answerId).style.height = 'auto'
            document.getElementById(answerId).style.transform = 'scale(1, 1)'
            document.getElementById(questionRect).style.backgroundColor = 'red'
            document.getElementById(questionRect).style.color = 'red'
            document.getElementById(questionRect).parentElement.children[1].style.backgroundColor = 'red'
        }
        function hideAnswer(e, plusId, answerId, questionRect) {
            e.target.style.display = 'none'
            document.getElementById(plusId).style.display = 'block'
            document.getElementById(answerId).style.transform = 'scale(1, 0)'
            document.getElementById(questionRect).style.backgroundColor = 'white'
            document.getElementById(questionRect).style.color = 'white'
            document.getElementById(questionRect).parentElement.children[1].style.backgroundColor = 'white'
            setTimeout(function() {
                document.getElementById(answerId).style.height = '0px'
            }, 300)
        }
