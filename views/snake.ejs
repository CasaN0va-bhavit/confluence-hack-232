    }
    .controls {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        padding-top: 30px;
    }
    .playArea {
        background-color: #222;
        width: 60vw;
        height: 50vh;
        clip-path: polygon(0% 0, 90% 0%, 100% 21%, 100% 100%, 0 100%);
