        align-items: center;
        justify-content: center;
        background-color: black;
    }
    .grid {
        display: flex;
        flex-direction: column;
        border: 1px solid #000;
        width: 100%;
        height: 100%;
    }
    .row {
        display: flex;
        flex-direction: row;
        flex: 1;
    }
    .box {
        /* border: 1px solid #000; */
        flex: 1;
